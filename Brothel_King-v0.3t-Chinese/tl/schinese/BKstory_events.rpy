translate schinese farm_meet_gizel2_fa209fb1:

    # templar "Seize her! And bring her accomplice too!"
    templar "逮捕她!也把她的同伙一起抓起来!"

# game/BKstory_events.rpy:1857
translate schinese farm_meet_gizel2_cb8bb2a0:

    # you "Hey! I'm not..."
    you "嘿!我不是..."

# game/BKstory_events.rpy:1859
translate schinese farm_meet_gizel2_416231f0:

    # initiate "Shut up. You're coming with us."
    initiate "闭嘴。你们跟我们走。"

